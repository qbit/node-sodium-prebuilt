{
  "name": "sodium-prebuilt",
  "version": "1.1.15",
  "author": "Pedro Paixao <paixaop@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "description": "Prebuilds for Lib Sodium port for node.js",
  "dependencies": {
    "nan": "^2.2.1",
    "prebuild": "^4.3.0"
  },
  "devDependencies": {
    "mdextract": "^1.0.0",
    "istanbul": ">=0.4.3",
    "mocha": ">=2.4.5",
    "mocha-istanbul": ">=0.2.0",
    "node-gyp": ">=3.3.1"
  },
  "scripts": {
    "test": "make test",
    "install": "prebuild --install --preinstall \"node install.js --preinstall && node install.js --install\"",
    "prebuild": "prebuild --all --preinstall \"node install.js --preinstall && node install.js --install\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mafintosh/node-sodium-prebuilt"
  },
  "keywords": [
    "encryption",
    "ed25519",
    "curve25519",
    "NaCl",
    "libsodium",
    "crypto",
    "unique",
    "stamp"
